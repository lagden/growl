  "scripts": {
    "pretest": "grunt build",
    "test": "grunt test",
    "start": "grunt serve"
  },
  "dependencies": {
    "lagden-utils": "^0.4.0"
  },
  "devDependencies": {
    "almond": "^0.3.1",
    "autoprefixer": "^6.0.3",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.0",
    "grunt-browser-sync": "^2.1.3",
    "grunt-concurrent": "^2.0.3",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-coffee": "^0.13.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-jade": "^0.15.0",
    "grunt-contrib-qunit": "^0.7.0",
    "grunt-contrib-requirejs": "^0.4.4",
    "grunt-contrib-stylus": "^0.22.0",
    "grunt-contrib-symlink": "^0.3.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-postcss": "^0.6.0",
    "grunt-xo": "^0.6.0",
    "jade": "^1.11.0",
    "jit-grunt": "^0.9.1",
    "requirejs": "^2.1.18",
    "time-grunt": "^1.2.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "esnext": true,
    "ignores": [
      "es6/lib/**"
    ],
    "global": [
      "window"
    ],
    "rules": {
      "arrow-parens": [
        1,
        "always"
      ],
      "new-cap": [
        2,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ]
    }
  }

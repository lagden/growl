  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "http://lagden.in"
  },
  "files": [
    "dist",
    "es6"
  ],
  "main": "dist/growl.js",
